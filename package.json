{
    "name": "monkey-parser",
    "version": "0.1.1",
    "description": "A Monkey Programming Language implementation in TypeScript",
    "author": "banqinghe",
    "license": "MIT",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "bin": "./dist/repl.cjs",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "repl": "tsx src/repl.ts",
        "test": "tsx --test",
        "test:watch": "tsx --test --watch"
    },
    "dependencies": {
        "picocolors": "^1.1.1"
    },
    "devDependencies": {
        "@stylistic/eslint-plugin": "^2.9.0",
        "@types/node": "^22.7.7",
        "@typescript-eslint/parser": "^8.10.0",
        "eslint": "^9.13.0",
        "tsx": "^4.19.1",
        "typescript": "^5.6.3",
        "unbuild": "^3.0.0-rc.11"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    }
}
